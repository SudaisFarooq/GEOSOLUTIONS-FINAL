{
  "version": 2,
  "name": "flood-help-app",
  "builds": [
    {
      "src": "public//*",
      "use": "@vercel/static"
    },
    {
      "src": "api//*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}